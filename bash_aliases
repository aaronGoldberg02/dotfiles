# aliases for convenience sake
alias ..='cd ..'
alias ...='cd ../..'
alias e='exit'
alias s='sudo'

# application aliases
alias vi='vim'
alias v='vim'
alias ymp3='youtube-dl --extract-audio --audio-format mp3 '
# packages
alias agu='sudo apt-get update'
alias agi='sudo apt-get install'
alias acs='apt-cache search'
alias agr='apt-get remove'

